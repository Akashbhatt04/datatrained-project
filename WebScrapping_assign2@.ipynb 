    "# Q:10 Go to webpage https://www.amazon.in/Enter “Laptop” in the search field and then click the search icon.Then set CPU Type filter to “Intel Core i7” and “Intel Core i9” as shown in the below image:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 139,
   "metadata": {},
   "outputs": [],
   "source": [
    "import selenium \n",
    "import pandas as pd\n",
    "from selenium import webdriver\n",
    "import time\n",
    "\n",
    "driver = webdriver.Chrome(r\"/Users/aks/Desktop/chromedriver\")\n",
    "\n",
    "url10=\"https://www.amazon.in/\"\n",
    "\n",
    "driver.get(url10)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 140,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Enter laptop\n",
    "driver.find_element_by_xpath(\"/html/body/div[1]/header/div/div[1]/div[2]/div/form/div[2]/div[1]/input\").send_keys('Laptop')\n",
    "\n",
    "## click on search\n",
    "driver.find_element_by_xpath(\"/html/body/div[1]/header/div/div[1]/div[2]/div/form/div[3]/div/span/input\").click()\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 141,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Apply filter i7 and i9\n",
    "driver.find_element_by_xpath(\"/html/body/div[1]/div[2]/div[1]/div[1]/div/div[3]/span/div[1]/span/div/div/div[6]/ul[2]/li[16]/span/a/div/label/i\").click()\n",
    "\n",
    "time.sleep(3)\n",
    "\n",
    "driver.find_element_by_xpath(\"/html/body/div[1]/div[2]/div[1]/div[1]/div/div[3]/span/div[1]/span/div/div/div[6]/ul[1]/li[18]/span/a/div/label/i\").click()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 142,
   "metadata": {},
   "outputs": [],
   "source": [
    "Title10=[]\n",
    "Rating10=[]\n",
    "Price_laptop=[]"
   ]
