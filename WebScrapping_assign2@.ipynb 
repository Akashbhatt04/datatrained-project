   "execution_count": 143,
   "metadata": {},
   "outputs": [],
   "source": [
    "Title_tag=driver.find_elements_by_xpath(\"//h2[@class='a-size-mini a-spacing-none a-color-base s-line-clamp-2']\")\n",
    "for i in Title_tag:\n",
    "    Title10.append(i.text)\n",
    "    \n",
    "time.sleep(3)\n",
    "    \n",
    "Rating_tag=driver.find_elements_by_xpath(\"//span[@class='a-icon-alt']\")\n",
    "for i in Rating_tag:\n",
    "    Rating10.append(i.text)\n",
    "        \n",
    "time.sleep(3)\n",
    "    \n",
    "Price_laptop_tag=driver.find_elements_by_xpath(\"//span[@class='a-price-whole']\")\n",
    "for i in Price_laptop_tag:\n",
    "    Price_laptop.append(i.text)"
   ]
